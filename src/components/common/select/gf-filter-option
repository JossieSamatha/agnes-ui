import {Option} from "element-ui";
import SpellFilter from "pinyin-match/dist/main"

export default {
    name: "GfFilterOption",
    extends: Option,
    methods: {
        queryChange: function (val) {
            this.visible = SpellFilter.match(this.currentLabel, val)
                || this.created, this.visible || this.select.filteredOptionsCount--
        }
    },
};
